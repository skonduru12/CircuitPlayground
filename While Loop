def main():
    print("Starting The Code Challenge")

    print("Starting Example 1")
    count = 1
    while count <= 5:
        print(count)
        count += 1
    print("Ending Example 1")

    print("Starting Example 2")
    user_num = 0
    while user_num != 7:
        user_num = int(input("Enter 7 to stop: "))
    print("Loop ended")
    print("Ending Example 2")

    print("Starting Example 3")
    x = 10
    while x > 0:
        print(x)
        x -= 1
    print("Blast off")
    print("Ending Example 3")

    print("Ending The Code Challenge")

main()
